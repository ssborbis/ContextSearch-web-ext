{
	"manifest_version": 2,
	"name": "ContextSearch web-ext",
	"description": "__MSG_extensionDescription__",
	"version": "1.45.3",
	"homepage_url": "https://github.com/ssborbis/ContextSearch-web-ext",
	"default_locale": "en",
	"applications": {
		"gecko": {
			"id": "{5dd73bb9-e728-4d1e-990b-c77d8e03670f}",
			"strict_min_version": "56.0"
		}
	},
	"background": {
		"scripts": [
			"lib/browser-polyfill.min.js",
			"lib/crossbrowser.js",
			"opensearch.js",
			"lib/encoding-indexes.js",
			"lib/encoding.min.js",
			"defaultEngines.js",
			"bookmarks.js",
			"defaultUserOptions.js",
			"contexts.js",
			"contextMenu.js",
			"searchActions.js",
			"nodes.js",
			"omnibox.js",
			"searchEngineUtils.js",
			"mozlz4.js",
			"keyTable.js",
			"utils.js",
			"background.js"
		]
	},
	"content_scripts": [
		{
			"matches": [
				"<all_urls>"
			],
			"js": [
				"lib/browser-polyfill.min.js",
				"lib/crossbrowser.js",
				"init_content.js"
			],
			"all_frames": true
		}
	],
	"options_ui": {
		"page": "options.html",
		"open_in_tab": true
	},
	"permissions": [
		"<all_urls>",
		"activeTab",
		"contextMenus",
		"storage",
		"tabs",
		"search",
		"unlimitedStorage"
	],
	"optional_permissions": [
		"bookmarks",
		"clipboardWrite",
		"clipboardRead",
		"nativeMessaging",
		"downloads"
	],
	"icons": {
		"48": "/icons/logo_notext.png"
	},
	"web_accessible_resources": [],
	"browser_action": {
		"default_title": "ContextSearch",
		"default_icon": {
			"48": "/icons/logo_notext.png"
		},
		"default_popup": "/searchbar.html"
	},
	"sidebar_action": {
		"default_icon": {
			"48": "/icons/logo_notext.svg"
		},
		"default_title": "ContextSearch web-ext",
		"default_panel": "blank.html",
		"open_at_install": false
	},
	"commands": {
		"_execute_browser_action": {},
		"_execute_sidebar_action": {}
	},
	"omnibox": {
		"keyword": "cs"
	}
}
